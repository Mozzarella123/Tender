@model IndexViewModel
@{
    ViewData["Title"] = "Profile";
    ViewData.AddActivePage(ManageNavPages.Index);
    ViewBag.Action = "Index";
}

<h4>@ViewData["Title"]</h4>
<div class="row">
    <div class="col-md-6">
        @Html.Partial("AccountPartials/_ProfileAccountPartial",Model)
    </div>
</div>
@Html.Partial("_ConfrirmationMessagePartial", new Dictionary<string, string>() { { "Message", "Вы действительно хотите удалить свой аккаунт? Действие необратимо." }, { "ButtonText", "Удалить" } })
@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}
