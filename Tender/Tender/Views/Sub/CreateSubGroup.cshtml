@using TenderApp.Models.BusinessModels
@model SubGroup

@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    Layout = "/Views/Shared/Layouts/_AdminLayout.cshtml";
}

@using (Html.BeginForm("CreateSubGroup","Admin"))

@using (Html.BeginForm())
{
    @Html.HiddenFor(m => m.SubGroupId);
    @Html.TextBoxFor(m => m.Name);
    @Html.GetEnumSelectList(Model.ForType.GetType());
    @Html.TextBoxFor(m => m.Priority);
    <input type = "submit" value = "Сохранить" />
}
